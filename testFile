package ICOS.pojo;

import java.util.List;
import java.util.Objects;

public class NewPersonRequest {

    private String strUuid;
    private String strSubType;
    private String strDomiCountry;
    private String strFirstName;
    private String strLastName;
    private String strGender;
    private List<String> lstNationalities;

    // Utility method to clean string inputs
    private String clean(String input) {
        return (input == null || input.trim().isEmpty()) ? null : input.trim();
    }

    // Getters and Setters
    public String getStrUuid() {
        return strUuid;
    }

    public void setStrUuid(String strUuid) {
        this.strUuid = clean(strUuid);
    }

    public String getStrSubType() {
        return strSubType;
    }

    public void setStrSubType(String strSubType) {
        this.strSubType = clean(strSubType);
    }

    public String getStrDomiCountry() {
        return strDomiCountry;
    }

    public void setStrDomiCountry(String strDomiCountry) {
        this.strDomiCountry = clean(strDomiCountry);
    }

    public String getStrFirstName() {
        return strFirstName;
    }

    public void setStrFirstName(String strFirstName) {
        this.strFirstName = clean(strFirstName);
    }

    public String getStrLastName() {
        return strLastName;
    }

    public void setStrLastName(String strLastName) {
        this.strLastName = clean(strLastName);
    }

    public String getStrGender() {
        return strGender;
    }

    public void setStrGender(String strGender) {
        this.strGender = clean(strGender);
    }

    public List<String> getLstNationalities() {
        return lstNationalities;
    }

    public void setLstNationalities(List<String> lstNationalities) {
        this.lstNationalities = lstNationalities;
    }
}

package ICOS.pojo;

import java.util.List;

public class ContactNoteRequest {

    private List<String> lstPersonIds;
    private String strSubject;
    private String strNotes;
    private boolean boolHasFollowup;

    // Utility method to clean string inputs
    private String clean(String input) {
        return (input == null || input.trim().isEmpty()) ? null : input.trim();
    }

    // Getters and Setters
    public List<String> getLstPersonIds() {
        return lstPersonIds;
    }

    public void setLstPersonIds(List<String> lstPersonIds) {
        this.lstPersonIds = lstPersonIds;
    }

    public String getStrSubject() {
        return strSubject;
    }

    public void setStrSubject(String strSubject) {
        this.strSubject = clean(strSubject);
    }

    public String getStrNotes() {
        return strNotes;
    }

    public void setStrNotes(String strNotes) {
        this.strNotes = clean(strNotes);
    }

    public boolean isBoolHasFollowup() {
        return boolHasFollowup;
    }

    public void setBoolHasFollowup(boolean boolHasFollowup) {
        this.boolHasFollowup = boolHasFollowup;
    }
}
