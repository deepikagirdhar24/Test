naturalPerson.setBlnHasTaxPrevClaimRemitanceBasis(
    payloadHelper.getBooleanValue(mapNaturalPersonDetails.get("hasTaxPrevClaimRemitanceBasis")));
naturalPerson.setBlnHasTaxIntendClaimRemitanceBasis(
    payloadHelper.getBooleanValue(mapNaturalPersonDetails.get("hasTaxIntendClaimRemitanceBasis")));
