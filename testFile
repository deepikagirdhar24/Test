// Add at class level
private static String TYK_API_KEY;

// Add this method inside TestConfig
public static String getTykApiKey() {
    if (TYK_API_KEY == null) {
        TYK_API_KEY = getSystemVariable("X-TYK-API-KEY");
    }
    return TYK_API_KEY;
}

 Update ContactNotes class:
String TYK_KEY = TestConfig.getTykApiKey();
