NewPersonRequest personRequest = new NewPersonRequest();
personRequest.setUuid(UUID.randomUUID().toString().substring(0,7));
personRequest.setSubType("pers_form_ass");
personRequest.setDomiCountry("CH");
personRequest.setFirstName(firstName);
personRequest.setLastName("Lastname");
personRequest.setGender("male");
personRequest.setNationalities(Collections.singletonList("CH"));

String jsonBody = objectMapper.writeValueAsString(personRequest);
