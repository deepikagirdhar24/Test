public POJOCreditRequest buildCreditPayload(Map<String, String> mapCreditDetails) {
    POJOCreditRequest creditRequest = new POJOCreditRequest();

    creditRequest.setStrBpId(payloadHelper.normalize(mapCreditDetails.get("bpId")));
    creditRequest.setStrMaccId(payloadHelper.normalize(mapCreditDetails.get("maccId")));

    return creditRequest;
}
