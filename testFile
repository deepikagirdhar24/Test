package ICOS.pojo;

import com.fasterxml.jackson.annotation.JsonProperty;

public class NewLegalPersonRequest {

    @JsonProperty("uuid")
    private String uuid;

    @JsonProperty("subType")
    private String subType;

    @JsonProperty("domiCountry")
    private String domiCountry;

    @JsonProperty("name")
    private String name;

    // Getters and Setters
    public String getUuid() {
        return uuid;
    }

    public void setUuid(String uuid) {
        this.uuid = uuid;
    }

    public String getSubType() {
        return subType;
    }

    public void setSubType(String subType) {
        this.subType = subType;
    }

    public String getDomiCountry() {
        return domiCountry;
    }

    public void setDomiCountry(String domiCountry) {
        this.domiCountry = domiCountry;
    }

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }
}
