    address.setStrFirstName(payloadHelper.normalize(mapAddressDetails.get("firstName")));
    address.setStrMiddleName(payloadHelper.normalize(mapAddressDetails.get("middleName")));
    address.setStrLastName(payloadHelper.normalize(mapAddressDetails.get("lastName")));
    address.setStrFirm(payloadHelper.normalize(mapAddressDetails.get("firm")));
